/* Custom CSS */
/* Custom sidebar menu */
.sidebar-wrapper {
  background: #FFF;
  width: var(--sidebar-width);
  position: fixed;
  padding: 0;
  top: 0;
  left: 0;
  bottom: 0;
  border-right: 1px solid var(--border-color);
  z-index: 9;
}
.sidebar-wrapper > div {
  display: flex;
  flex-direction: column;
  height: 100%;
  overflow-y: auto;
}
@media screen and (min-width: 1025px) {
  .sidebar-wrapper > div {
    overflow: -moz-scrollbars-none;
    -ms-overflow-style: none;
    scrollbar-width: none;
  }
  .sidebar-wrapper > div::-webkit-scrollbar {
    width: 0 !important;
    height: 0 !important;
  }
}
.ss-header-top {
  background: #FFF;
  position: sticky;
  top: 0;
  border-bottom: 1px solid var(--border-color);
  z-index: 10;
}
.ss-header-top .main_hdr {
  padding: 9px 15px 11px;
}
.main-wrapper__content {
  position: relative;
}
.sidebar-content {
  padding: 0 10px;
}
.sidebar-content .header-logo {
  padding: 13px 0 21px 0px;
  max-width: none;
}
.sidebar-menu-list li a {
  font-size: 12px;
  line-height: 38px;
  font-weight: 600;
  color: var(--color-text);
  padding: 0 10px 0 17px;
  display: flex;
  align-items: center;
  gap: 12px;
}
.sidebar-menu-list > li > a {
  border-radius: 7px;
}
.sidebar-menu-list > li > a svg {
  width: 16px;
  height: auto;
}
.sidebar-menu-list > li > a.active,
.sidebar-menu-list > li > a:hover {
  background: var(--bgr-light);
  color: var(--color-main-text);
}

/* Sidebar submenu */
.sidebar-submenu {
  margin-top: 22px;
  padding: 22px 10px;
  position: relative;
  border-top: 1px solid var(--border-color);
}
.sidebar-menu-footer {
  margin-top: auto;
  padding: 33px 0 29px 34px;
  position: relative;
  border-top: 1px solid var(--border-color);
}
.sidebar-menu-footer a {
  font-size: 12px;
  line-height: 18px;
  font-weight: 600;
  color: var(--color-main-text);
  gap: 12px;
}

/* Search */
.search .s_input {
  background: var(--bgr-light);
  padding: 0 25px 0 35px;
  border-radius: 8px;
  font-weight: 500;
  border: 2px solid var(--bgr-light);
}
.search .s_input:focus {
  border-color: var(--color-main);
}
.search .s_input::placeholder {
  color: #BDBDBD;
}
.search .s_submit {
  position: absolute;
  left: 14px;
  top: 0;
  padding: 0;
  background: transparent;
  bottom: 0;
  display: flex;
  align-items: center;
}
@media screen and (min-width: 769px) {
  .ss-header-top .main_hdr {
    padding: 21px 30px 19px 30px;
  }
  .main-wrapper__content {
    margin-left: var(--sidebar-width);
  }
  .sidebar-content .header-logo {
    padding: 23px 0 34px 2px;
  }
}
@media screen and (max-width: 768px) {
  .sidebar-wrapper {
    transform: translateX(-115%);
    transition: all 0.4s ease-in-out;
    z-index: 668;
  }
  .actMbNav .sidebar-wrapper {
    transform: translateX(0);
  }
  .close-sidebar {
    position: absolute;
    top: 18px;
    right: 10px;
  }
}
@media screen and (max-width: 1024px) {
  .sidebar-menu-footer {
    padding: 25px 0 25px 34px;
  }
  .collection-list2 .cl_box .swiper {
    margin-right: 0;
    padding-right: 78px;
  }
}
@media screen and (max-width: 768px) {
  .ss-header-top .main_hdr {
    justify-content: space-between;
  }
  .button-open-menu, .ss-header-top .icons-col {
    flex: 0 0 100px;
  }
  .button-open-menu a {
    display: block;
    line-height: 1;
    margin-top: 1px;
  }
}
@media screen and (max-width: 567px) {
  :root {
    --sidebar-width: 215px;
  }
}