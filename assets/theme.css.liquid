{% comment %}/* <style> */{% endcomment %}
.placeholder-svg { display:block; fill:{{ settings.bodycl | color_modify:'alpha', 0.2 }}; background-color:#f4f4f4; width:100%; height:100%; max-width:100%; max-height:100%; opacity:0.8; }
