 .sec__pdp-bottom {
  border-top: 1px solid var(--border-color);
  border-bottom: 1px solid var(--border-color);
  margin-bottom: 10px;
  padding-bottom: 20px;
}
.prd-d-rv .review-box {
  padding: 30px 0 30px 30px;
  box-sizing: border-box;
} 
.sec__pdp-bottom .gitem.ss-right-box {
  padding: 20px 5px 0 5px;
  box-sizing: border-box;
}
.rte ul li::marker {
  color: rgba(255, 155, 67, 1);
}
.note-bd {
  border: 1px dashed rgba(174, 174, 174, 1);
  border-radius: 8px;
  padding: 20px 13px;
}
.note-bd ul {margin: 12px 0 30px 15px;}
.rte ul.mb0 {
  margin-bottom: 0;
}
.rte {
  color: var(--color-main-text);
}
.rte p {
  font-size: 14px;
  line-height: 21px;
}
.rte strong {font-weight: 500;}

.pr_btn.product-form__submit.loading .loading__spinner {
  display: block;height: auto;visibility: visible;
}
.pr_btn.product-form__submit.loading .txt {height: auto;}

.sec__pdp-bottom .gitem.ss-left-box {
  padding-top: 20px;
}

.product-single-tee .product__images-wrapper h3 {font-size: 16px;line-height: 26px;margin-bottom: 15px;}
.product-single-tee .product__images-wrapper .item {
  min-width: 115px;
  width: 115px;
  border-radius: 8px;
  overflow: hidden;
}

.product-single-tee>.col-media {
  border-bottom: 1px solid var(--border-color);
  padding-bottom: 14px;
  margin-bottom: 30px;
}

@media only screen and (min-width:577px) {
  .product-single-tee>.col-media {
    position: sticky;
    top: 0;
    border: none;
    padding-bottom: 0;
    margin-bottom: 0;
  }
  .sec__pdp-bottom .fw-sec > .grid {
    display: grid;
    gap: 0;
    grid-template-columns: repeat(2, minmax(0, 1fr));
    margin: 0;
  }
}
@media only screen and (min-width:769px) {
  .product-single-tee .product__images-wrapper h3 {
    display: none;
  }
  .product-single-tee {
    flex-direction: row;
    margin: 0;
  }
  .product-single-tee>.col-media {
    display: grid;
    grid-template-columns: 256px minmax(0,1fr);
    grid-template-areas: "area11 area22";
    align-items: start;
    height: 100%;
  }
  .product-single-tee .product__images-wrapper .list-item-images {
    width: auto;
    display: grid;
    grid-template-columns: repeat(2, 1fr);
    gap: 10px;
    grid-area: area11;
  }
  .product-single-tee .product__media-wrapper {
    width: auto;
    grid-area: area22;
  }
  .product-single-tee .product__info {
    width: 100%;
  }
  .sec__pdp-bottom {
    margin-bottom: 15px;
  }
}

@media only screen and (min-width:769px) and (max-width:1024px) {
  .product-single-tee>.col-media {
    position: relative;
  }
  .product-single-tee .product__info {
    padding: 30px 0 0 0;
    margin-top: 30px;
    position: relative;
  }
  .product-single-tee .product__info:after {
    content: "";
    position: absolute;
    top: 0;
    width: auto;
    height: 1px;
    background-color: var(--border-color);
    left: -30px;
    right: -30px;
  }
  .product-single-tee product-info {
    display: grid;
    grid-template-columns: repeat(2, minmax(0, 1fr));
    gap: 30px;
  }
  .product-single-tee product-info .list-item-info {
    grid-column: 1;
  }
  .product-single-tee product-info > .ctabtn {
    grid-column: 2;
  }
  .product__info variant-selects {
    margin-bottom: 0;
  }
  .product__info variant-selects .pvOpt2 {
    margin-bottom: 0;
  }
}

@media only screen and (min-width:1025px) {
  .note-bd {
    padding: 30px;
  }
  .note-bd ul {margin: 20px 0 30px 15px;}
  .sec__pdp-bottom {
    padding-bottom: 0;
    margin-bottom: 30px;
  }
  .sec__pdp-bottom .gitem.ss-left-box {
    padding: 30px 30px 30px 0;
    border-right: 1px solid var(--border-color);
    box-sizing: border-box;
  }
  .sec__pdp-bottom .gitem.ss-right-box {
    padding: 30px 0 30px 30px;
    box-sizing: border-box;
  }
  .product-single-tee {
    display: flex;
    flex-wrap: nowrap;
    gap: 30px;
    margin: 0;
    align-self: stretch;
  }
  .product-single-tee > .gitem {
    padding-left: 0;
    padding-right: 0;
  }
  .product-single-tee .product__images-wrapper .list-item-images {
    gap: 16px;
  }
  .product-single-tee .product__images-wrapper .item {
    border-radius: 8px;
    overflow: hidden;
  }
  .product-single-tee>.col-media {
    position: sticky;
    padding-right: 30px;
    flex: 1 1 auto;
    height: auto;
    min-width: 0;
  }
  .product-single-tee>.col-media:after {
    content: "";
    position: absolute;
    top: -30px;
    right: 0;
    width: 1px;
    height: auto;
    bottom: -30px;
    background-color: var(--border-color);
  }
  .product-single-tee .product__info {
    width: auto;
    flex: 0 0 auto;
    min-width: 350px;
    max-width: 350px;
  }
  .product-single-tee .product__info .wrap-product-info {
    margin-left: 0;
  }
}

@media only screen and (min-width:577px) and (max-width:768px) {
  .product-single-tee {
    gap: 15px;
    margin: 0;
  }
  .product-single-tee > div {
    flex: 1;
    min-width: 0;
  }
  .product-single-tee .product__images-wrapper {
    width: auto;
  }
  .product-single-tee .product__media-wrapper {
    width: auto;
  }
  .product-single-tee .product__info {
    width: auto;
  }
}

@media only screen and (min-width:1025px) and (max-width:1500px) {
  .product-single-tee>.col-media {
    grid-template-columns: 115px minmax(0, 1fr);
  }
  .product-single-tee .product__images-wrapper .list-item-images {
    grid-template-columns: 1fr;
  }
}
@media only screen and (min-width:1600px) {
  .product-single-tee .product__info {
    min-width: 525px;
    max-width: 600px;
  }
}
@media only screen and (min-width:2450px) {
  .sec_cov {
    max-width: 2400px;
    margin: 0 auto;
  }
}

/* Custom photoswipe thumbs */
.pswp__scroll-wrap .pswp__item img {
  background: #000;
  object-fit: contain;
  padding-bottom: 70px;
}
.pswp-thumbs {
  background: #000;
  position: absolute;
  left: 0;
  right: 0;
  bottom: 0;
  height: 70px;
  padding-top: 10px;
  z-index: 10000;
  pointer-events: auto;
}
.pswp-thumbs .swiper-wrapper {
  justify-content: center;
}
.pswp__scroll-wrap .pswp__counter {
  left: 20px;
  margin: 0;
  top: 10px;
  min-width: 10px;
  padding: 0;
  background: transparent;
  color: #FFF;
}
.pswp__scroll-wrap .pswp__button--arrow {
  top: 50%;
  bottom: auto;
  margin: 0;
  padding: 0;
  min-width: 10px;
  display: flex;
  align-items: center;
  justify-content: center;
  background: #FFF;
  border-radius: 100%;
  width: 30px;
  height: 30px;
}
.pswp__scroll-wrap .pswp__button--arrow--prev {
  left: 20px;
}
.pswp__scroll-wrap .pswp__button--arrow--next {
  right: 20px;
  left: auto;
}
.pswp__scroll-wrap .pswp__button--arrow .pswp__icn {
  fill: #FFF;
}
.pswp-thumbs .swiper {
  height: 100%;
}
.pswp-thumbs .swiper-slide {
  width: 60px;
  height: 60px;
  opacity: 0.5;
  cursor: pointer;
}
.pswp-thumbs .swiper-slide-thumb-active {
  opacity: 1;
  outline: 2px solid #fff;
}
.pswp-thumbs img {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

/* Tee form */
div.tee-dialog, div.tee-form-wrapper, div.tee-gallery,
.teeinblue-theme-modern .tee-dialog, .teeinblue-theme-modern .tee-form-wrapper {
  --tee-primary-lighten-color: var(--color-main);
  --tee-preview-button-text-color: var(--color-main);
  --tee-form-header-color: var(--color-main-text);
  --tee-font-size: 12px;
  --tee-field-heading-font-size: 12px;
  --tee-field-select-color: var(--color-main-text);
  --tee-customize-main-form-border-color: #DDDDDD;
  --tee-form-input-height: 40px;
  --tee-atc-border-radius: 8px;
  --tee-clipart-border-radius: 8px;
  --tee-form-input-border-radius: 8px;
  --tee-form-input-height: 40px;
  --tee-clipart-active-border-width: 1px;
  --tee-input-active-border-width: 1px;
  --tee-field-select-focus-border-width: 1px;
  --tee-form-input-color: var(--color-main-text);
  --tee-filed-select-focus-padding: 7px 35px 7px 15px;
}

.teeinblue-theme-modern .tee-customize-main-form {border: none;margin-bottom: 20px;}
.tee-form-header {background-color: transparent;padding: 0 0 15px 0;text-align: left;}
.tee-form-inputs {padding: 0;}
.tee-field__heading {font-weight: 500 !important;}
.tee-field__select:focus-visible,
.tee__input:focus-visible {box-shadow: none;}
.tee-btn, .tee-song__btn {font-weight: 500;}
.tee-clipart-row {
  margin: 0;
  width: 100%;
  gap: 10px;
}
.tee-clipart-col {
  padding: 0;
  flex: 0 0 auto;
  max-width: none;  
  min-width: 60px;
}
.teeinblue-theme-modern .tee-clipart-checkmark {
  border: none;
  width: 11px;
  height: 11px;
  top: auto;
  bottom: 4px;
  right: 4px;
}

.tee-field.tee-field--text {
  display: flex;
}
.tee-field {display: flex;flex-wrap: wrap;}
.tee-field > div {width: 100%;}
.tee-field .tee-field__input {flex: 1;}
.tee-field .tee-field__input + .tee-field__subfield {
  flex: 0 0 40px; 
  padding-left: 10px;
  width: 40px;
  margin-top: 0;
  box-sizing: content-box;
}
.tee-field .tee-field__input + .tee-field__subfield .tee-field__header {display: none;}
.tee-colorpicker-handle {border-radius: 8px;box-shadow: none;border: 1px solid var(--tee-customize-main-form-border-color);}
.tee-colorpicker-body {left: auto;right: 0;}
.tee-switch__input {padding-top: 10px;padding-bottom: 10px;}
.teeinblue-theme-modern .tee-thumbnails__button {display: none;}
.tee-btn--preview {height: 40px;}
.tee-campaign-form-footer .tee-block {
  margin-bottom: 10px;
}
@media only screen and (min-width:769px){
  .tee-btn-wrapper--personalize {
    width: calc(100vw - var(--sidebar-width));
    left: auto;
    right: 0;
  }
}

@media only screen and (max-width:768px) {
  .product-single-tee .product__images-wrapper .list-item-images {
    gap: 10px;
    display: flex;
    overflow-x: auto;
    padding: 0;
  }
  .pswp__top-bar .pswp__button--close {
    top: 10px;
    right: 10px;
  }
  .pswp__scroll-wrap .pswp__counter {
    top: 0;
  }
  .pswp__scroll-wrap .pswp__button--arrow {transform: none;}
  .pswp__scroll-wrap .pswp__button--arrow--prev {left: 5px;}
  .pswp__scroll-wrap .pswp__button--arrow--next {right: 5px;}
  .product-single-tee > .col-media {
    display: flex;
    flex-direction: column;
    gap: 30px;
  }

  .product__images-wrapper {
    order: 1;
  }
}
