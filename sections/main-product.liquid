							{%- liquid
								assign vname = product.vendor | url_param_escape
								assign vlink = routes.collections_url | append: '/vendors?q=' | append: vname
								for coll in product.collections
									if coll.title == product.vendor
										assign vlink = coll.url
										break
									endif
								endfor
								assign img_name = product.vendor | handleize | append:'.png'
								assign img_src = img_name | file_img_url: '200x'
