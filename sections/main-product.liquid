								{% if img_src contains '?v=' and blk_st.img %}
									{% # theme-check-disable RemoteAsset %}
									<img class="vendImg" loading="lazy" src="{{ img_src }}" width="{{ blk_st.lwd }}" height="100" alt="{{ product.vendor }}">
									{% # theme-check-enable RemoteAsset %}
								{% else %}
									{{ product.vendor }}
								{% endif %}
							</a>
						</div>
					{%- when 'variants' -%}
