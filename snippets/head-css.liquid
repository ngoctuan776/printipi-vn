{% comment %}theme-check-disable{% endcomment %}
{%- capture sitecss -%}
{%- liquid
    if is_rtl
        assign css_file = 'themeRtl.css'
        assign p_left = 'right'
        assign p_right = 'left'
    else
        assign css_file = 'theme.css'
        assign p_left = 'left'
        assign p_right = 'right'
    endif 
-%}
<link rel="preload" as="style" href="{{ css_file | asset_url }}">
{% if settings.fonts == '1' %}
    {% liquid
    	assign font1 = settings.font1
        assign ft1 = font1.family
     	assign font2 = settings.font2 | default:font1
        assign ft2 = font2.family
		assign font3 = settings.font3 | default:font1
        assign ft3 = font3.family
    %}
    {%- for variant in font1.variants -%}
        {% if variant.weight == "400" or variant.weight == "700" and variant.style != "italic" %}
           {% if variant.style != "italic" %}<link rel="preload" as="font" href="{{ variant | font_url }}" type="font/woff2" crossorigin>{% endif %}
        {% endif %}
    {%- endfor -%}
    {%- if ft2 != ft1 -%}
        {% for variant in font2.variants %}    
            {% if variant.weight == "400" or variant.weight == "700" and variant.style != "italic" %}
               {% if variant.style != "italic" %}<link rel="preload" as="font" href="{{ variant | font_url }}" type="font/woff2" crossorigin>{% endif %}
            {% endif %}
        {% endfor %}
    {%- endif -%}
    {%- if ft3 != ft1 -%}    
        {% for variant in font3.variants %}    
            {% if variant.weight == "400" or variant.weight == "700" and variant.style != "italic" %}
               {% if variant.style != "italic" %}<link rel="preload" as="font" href="{{ variant | font_url }}" type="font/woff2" crossorigin>{% endif %}
            {% endif %}
        {% endfor %}
    {%- endif -%}

    {% capture font_css %}
        {%- for variant in font1.variants -%}    
            {% if variant.weight > "200" and variant.weight < "800" and variant.style != "italic" %}{{ variant | font_face:font_display:'swap' | replace:": ",":" | replace:" format","format" }}{% endif %}
        {%- endfor -%}
        {%- if ft2 != ft1 -%}
            {% for variant in font2.variants %}
                {% if variant.weight > "200" and variant.weight < "800" and variant.style != "italic" %}{{ variant | font_face:font_display:'swap' | replace:": ",":" | replace:" format","format" }}{% endif %}    
            {% endfor %}
        {%- endif -%}
        {%- if ft3 != ft1 -%}
            {% for variant in font3.variants %}    
                {% if variant.weight > "200" and variant.weight < "800" and variant.style != "italic" %}{{ variant | font_face:font_display:'swap' | replace:": ",":" | replace:" format","format" }}{% endif %}
            {% endfor %}
        {%- endif -%}
    {% endcapture %}
{% else %}
	{% comment %} <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    {% liquid
    	assign font1 = settings.gfont1 | strip | replace:' ', '+'
     	assign font2 = settings.gfont2 | strip | replace:' ', '+'
		assign font3 = settings.gfont3 | strip | replace:' ', '+'
    %}
    {%- capture font_names -%}{{ font1 }},{{ font2 }},{{ font3 }}{%- endcapture -%}    
    {%- assign font_names = font_names | split:',' | uniq | join:',' | split:',' -%}    
    {%- capture gfonts -%}    
        {%- for font in font_names -%}family={{ font }}:wght@300;400;500;600;700{% unless forloop.last %}&{% endunless %}{% endfor %}
    {%- endcapture -%}
    <link href="https://fonts.googleapis.com/css2?{{ gfonts }}&display=swap" rel="stylesheet" media="print" onload="this.media='all'">     {% endcomment %}
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter:ital,opsz,wght@0,14..32,400;0,14..32,500;0,14..32,600;1,14..32,400;1,14..32,500;1,14..32,600&display=swap" rel="stylesheet">
{% endif %}

<style type="text/css">
:root{    
    {% if settings.fonts == '1' %}
        --ft1:{{ settings.font1.family }},{{ settings.font1.fallback_families }};
        --ft2:{{ settings.font2.family | default:settings.font1.family }},{{ settings.font2.fallback_families }};
        --ft3:{{ settings.font3.family | default:settings.font1.family }},{{ settings.font3.fallback_families }};    
    {% else %}
        --ft1:{{ settings.gfont1 | strip }};
        --ft2:{{ settings.gfont2 | strip | default:settings.gfont1 | strip }};
        --ft3:{{ settings.gfont3 | strip | default:settings.gfont1 | strip }};
    {% endif %}
    --site_gutter:15px;
    --grid_gutter:15px;
    --grid_guttert:10px;
    --grid_gutterm:5px;
    --input_height:40px;
    --input_height_sm:34px;
    --error:#d20000;
    --errorbg:#fadfdf;
    --arw:{{ settings.arrow }};
    --arwbg:{{ settings.arrowbg }};
    --dots:{{ settings.dots }};
    --dotsa:{{ settings.dotsa }};
}

